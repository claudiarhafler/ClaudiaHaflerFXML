        </columns>
      </TableView>
      <Button fx:id="Search" layoutX="249.0" layoutY="198.0" mnemonicParsing="false" onAction="#Clicked" text="Search" />
      <TextField layoutX="14.0" layoutY="198.0" prefHeight="27.0" prefWidth="225.0" />
      <Text layoutX="31.0" layoutY="194.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Search Workouts" />
      <Button layoutX="204.0" layoutY="230.0" mnemonicParsing="false" onAction="#searchByNameAdvancedAction" text="Advanced Search" />
    </children>
</AnchorPane>
