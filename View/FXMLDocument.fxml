<AnchorPane id="AnchorPane" prefHeight="514.0" prefWidth="322.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="126.0" layoutY="148.0" onAction="#handleButtonAction" text="Click Me!" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <Button fx:id="createButton" layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#createWorkout" text="Create" />
      <Button fx:id="readButton" layoutX="92.0" layoutY="26.0" mnemonicParsing="false" onAction="#readWorkout" text="Read" />
      <Button fx:id="updateButton" layoutX="175.0" layoutY="26.0" mnemonicParsing="false" onAction="#updateWorkout" text="Update" />
      <Button fx:id="deleteButton" layoutX="251.0" layoutY="26.0" mnemonicParsing="false" onAction="#deleteWorkout" text="Delete" />
      <Button fx:id="repsandSets" layoutX="65.0" layoutY="100.0" mnemonicParsing="false" onAction="#getRepsAndSets" text="Reps and Sets" />
